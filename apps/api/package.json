{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "dev": "cross-env NODE_ENV=local nest start --watch",
    "predev": "pnpm migration:run",
    "start:dev": "cross-env NODE_ENV=local node dist/apps/api/main",
    "start:prod": "cross-env NODE_ENV=production node dist/apps/api/main",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/configs/typeorm/typeorm.cli.config.ts",
    "migration:create": "cross-env NODE_ENV=local typeorm-ts-node-commonjs migration:create ./src/shared/infrastructure/persistence/migrations/migration -t $(node -e \"const d = new Date(Date.now() + 9*60*60*1000); console.log(d.toISOString().replace(/[^0-9]/g, '').slice(0, 14))\")",
    "migration:generate": "cross-env NODE_ENV=local pnpm typeorm migration:generate ./src/shared/infrastructure/persistence/migrations/migration -pt $(node -e \"const d = new Date(Date.now() + 9*60*60*1000); console.log(d.toISOString().replace(/[^0-9]/g, '').slice(0, 14))\")",
    "migration:run": "cross-env NODE_ENV=local pnpm typeorm migration:run",
    "migration:revert": "cross-env NODE_ENV=local pnpm typeorm migration:revert",
    "migration:show": "cross-env NODE_ENV=local pnpm typeorm migration:show",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:auth": "jest --auth",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "create:module": "bash ../../scripts/create-module.sh",
    "test:module": "jest --testPathPattern",
    "analyze:module": "echo 'Analyzing module structure...'"
  },
  "dependencies": {
    "@keyv/redis": "^4.3.2",
