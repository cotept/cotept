{
  "name": "@repo/api-client",
  "version": "0.0.1",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "openapi-generator-cli generate -i ./openapi-spec.yaml -g typescript-axios -o ./src -c ./config.json -t ./templates/mustache",
    "gapi": "rm -rf ./src && docker run --rm -v \"${PWD}:/local\" openapitools/openapi-generator-cli generate -i /local/openapi-spec.yaml -g typescript-axios -o /local/src/api -c /local/config.json",
    "test": "docker run --rm -v /c/Users/jtm99/project/cotept-update/cotept/packages/api-client:/local openapitools/openapi-generator-cli generate -i /local/openapi-spec.yaml -g typescript-axios -o /local/src/api -t /local/mustaches -c /local/config.json --global-property=apiDocs=false,modelDocs=false"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.18.4"
  }
}
